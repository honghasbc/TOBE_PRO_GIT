<script type="text/javascript">
var oTable;
$(document).ready(function() {
    /* Init the table */
     $('#dynamic').html( '<table cellpadding="0" cellspacing="0" width ="100%" border="0" class="display" id="example"></table>' );
    oTable = $('#example').dataTable({
		"bPaginate": false,
        "bLengthChange": false,
        "bFilter": false,
        "bSort": false,
        "bInfo": false,
        "bAutoWidth": false,
		"sAjaxSource": 'sources/arrays.txt',
        "bAutoWidth": false,
        "fnRowCallback": function( nRow, aData, iDisplayIndex ) {
            /* Append the grade to the default row class name */
            $('td:eq(9)', nRow).html( '<img class =\"imageEditInTable\" src=\"images/editaction.png\"> <img class =\"imageViewInTable\" src=\"images/view.png\">');
        },
        "oTableTools": {
            "sRowSelect": "single"
        },
        "oLanguage": {
            "sLengthMenu": 'Show <select>'+
              '<option value="10">10</option>'+
              '<option value="20">20</option>'+
              '<option value="30">30</option>'+
              '<option value="40">40</option>'+
              '<option value="50">50</option>'+
              '<option value="-1">All</option>'+
              '</select> rows'
        },
        "aoColumns": [
                      { "sTitle": "Customer ID", "sClass": "center", "sWidth": "5%"  },
                      { "sTitle": "Customer Name", "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "Phone Number" , "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "Avg Volume($ amount)", "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "CSS grade",  "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "CSS grade",  "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "CSS grade",  "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "CSS grade",  "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "CSS grade",  "sClass": "center", "sWidth": "10%" },
                      { "sTitle": "Action",  "sClass": "center", "sWidth": "5%" }
                  ]
	});
    $('#example').dataTable();
//     Set some properties for table 
    /* Add a click handler to the rows - this could be used as a callback */
    $('#example tbody').on( 'click', 'tr', function () {
    	 if ( $(this).hasClass('row_selected') ) {
             $(this).removeClass('row_selected');
         }
         else {
             oTable.$('tr.row_selected').removeClass('row_selected');
             $(this).addClass('row_selected');
         }
    });
    $('#example tbody').on( 'click', 'td', function () {
    	if ($(this).find('.imageViewInTable').length > 0) {
			var data = oTable.fnGetData($(this).parent("tr")[0]);
			alert("Function is under contruction");
// 			$.fn.comfirmNavigate(data[0]);
//     		var oTT = TableTools.fnGetInstance('example');
// 	        var aData = oTT.fnGetSelectedData();
// 	     	oTT.fnSelect( $('#example tbody tr')[0] );
			
   		}else if ($(this).find('.imageEditInTable').length > 0) {
			var data = oTable.fnGetData($(this).parent("tr")[0]);
			alert("Function is under contruction");
   		}
    	
    });
    $('input#labelRight,input#labelLongRight').each(function(){
        $(this).attr("disabled", "disabled");
	});
    $(".imgEditCusInfo").click(function () {
		$('input#labelRight,input#labelLongRight').each(function(){
			$('#customerInfomation').find($(this)).css("border","1px solid #D3D3D3");
			$('#customerInfomation').find($(this)).removeAttr("disabled");
	    });
    });
	$(".imgMaxInfo").click(function () {
// 		$.fn.changeImgMaxMin($(this));
		var src = $(this).attr('src');
		if (src.match(/maximize-all-2.png$/)) {
			src = src.replace("maximize", "minimize");
			$("#customerInfomation").hide("blind");
		}else {
			src = src.replace("minimize", "maximize");
			$("#customerInfomation").show("blind");
		} 	
		$(this).attr("src",src);
		
	});
	
	$(".imgEditBilInfo").click(function () {
		$('input#labelRight,input#labelLongRight').each(function(){
			$('#billingInfor').find($(this)).css("border","1px solid #D3D3D3");
			$('#billingInfor').find($(this)).removeAttr("disabled");
	    });
	});
	
	$(".imgMaxBilInfo").click(function () {
		var src = $(this).attr('src');
		if (src.match(/maximize-all-2.png$/)) {
			src = src.replace("maximize", "minimize");
			$("#billingInfor").hide("blind");
		}else {
			src = src.replace("minimize", "maximize");
			$("#billingInfor").show("blind");
		} 	
		$(this).attr("src",src);
	});
});
</script>
